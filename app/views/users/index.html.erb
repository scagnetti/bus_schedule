<div class="row">
  <div class="col-md-4">
    <h1>Listing users</h1>
  </div>
</div>
 
 <div class="row">
  <div class="col-md-4">
    <table class="table table-striped table-bordered">
      <tr>
        <th>email</th>
        <th>password</th>
      </tr>
     
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td>******</td>
        </tr>
      <% end %>
    </table>
  </div>
</div>

 <div class="row">
  <div class="col-md-4">
    <%= link_to 'New user', new_user_path %>
  </div>
</div>