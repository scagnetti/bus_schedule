 <h1>Listing users</h1>

<table class="table table-striped table-bordered">
  <tr>
    <th>email</th>
    <th>password</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td>******</td>
    </tr>
  <% end %>
</table>

<%= link_to 'New user', new_user_path %>
